pipeline {
    agent any

    options {
        buildDiscarder logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '5', daysToKeepStr: '', numToKeepStr: '5')
    }

    stages {
        stage('Build on DEV') {
            steps {
                script {
                    echo "Hello Jenkins pipeline"
                    sh "ls"
                    sh "pwd"
                    buildFile = libraryResource 'com/bookatop/scripts/build-backeend-on-dev.sh'
                    sh buildFile
                }
            }
        }
    }
}